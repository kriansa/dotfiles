{
  "$schema" : "https:\/\/schema.linearmouse.app\/0.10.2",
  "schemes" : [
    {
      "if" : {
        "device" : {
          "category" : "trackpad",
          "productID" : "0x0",
          "productName" : "Apple Internal Keyboard \/ Trackpad",
          "vendorID" : "0x0"
        }
      },
      "pointer" : {
        "disableAcceleration" : false
      }
    },
    {
      "buttons" : {
        "clickDebouncing" : {
          "timeout" : 0
        }
      },
      "if" : {
        "device" : {
          "category" : "mouse",
          "productID" : "0x84",
          "productName" : "Razer DeathAdder V2",
          "vendorID" : "0x1532"
        }
      },
      "pointer" : {
        "acceleration" : 2.5,
        "disableAcceleration" : false,
        "redirectsToScroll" : false,
        "speed" : 0.05
      },
      "scrolling" : {
        "acceleration" : {
          "vertical" : 1
        },
        "distance" : {
          "vertical" : 3
        },
        "reverse" : {
          "vertical" : true
        },
        "speed" : {
          "vertical" : 0
        }
      }
    }
  ]
}